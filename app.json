{
  "expo": {
    "name": "Debt Disha",
    "slug": "deptdisha",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/optimized/app-logo.webp",
    "scheme": "deptdisha",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/optimized/splash.webp",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "assetBundlePatterns": [
      "assets/images/optimized/*.webp",
      "assets/fonts/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.deptdisha.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/optimized/app-logo.webp",
        "backgroundColor": "#121212"
      },
      "package": "com.deptdisha.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/optimized/app-logo.webp"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a3a29a31-c51f-4b03-a955-7ef0380e397f"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/a3a29a31-c51f-4b03-a955-7ef0380e397f"
    },
    "owner": "priyanshuakafenix"
  }
}
